{
  "scriptFile": "main.py",
  "bindings": [
    {
      "type": "httpTrigger",
      "direction": "in",
      "name": "req"
    },
    {
      "type": "blob",
      "direction": "out",
      "name": "file",
      "dataType": "string",
      "connection": "AzureWebJobsStorage",
      "path": "python-worker-perf-tests/{outfile}"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    }
  ]
}
