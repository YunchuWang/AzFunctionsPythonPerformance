{
  "scriptFile": "main.py",
  "bindings": [
    {
      "type": "httpTrigger",
      "direction": "in",
      "name": "req"
    },
    {
      "type": "blob",
      "direction": "in",
      "name": "inputfile1",
      "dataType": "binary",
      "connection": "AzureWebJobsStorage",
      "path": "python-worker-perf-tests/{infile1}"
    },
    {
      "type": "blob",
      "direction": "in",
      "name": "inputfile2",
      "dataType": "binary",
      "connection": "AzureWebJobsStorage",
      "path": "python-worker-perf-tests/{infile2}"
    },
    {
      "type": "blob",
      "direction": "out",
      "name": "outputfile1",
      "dataType": "binary",
      "connection": "AzureWebJobsStorage",
      "path": "python-worker-perf-tests/{outfile1}"
    },
    {
      "type": "blob",
      "direction": "out",
      "name": "outputfile2",
      "dataType": "binary",
      "connection": "AzureWebJobsStorage",
      "path": "python-worker-perf-tests/{outfile2}"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    }
  ]
}
